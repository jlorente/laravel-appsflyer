    "keywords": [
        "php",
        "jlorente",
        "laravel",
        "appsflyer"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "JosÃ© Lorente",
            "email": "jose.lorente.martin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.5",
        "jlorente/appsflyer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Jlorente\\Laravel\\Appsflyer\\": "src/"
        }
